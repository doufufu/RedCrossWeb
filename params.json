mage.height; \r\n                }\r\n                //alert(w)\r\n                //alert(h)\r\n                $(imgObj).css(\"margin-left\",parseInt((widthValue-w)/2)+\"px\");\r\n                $(imgObj).css(\"margin-top\",parseInt((heightValue-h)/2)+\"px\");\r\n                $(imgObj).css(\"width\",parseInt(w)+\"px\");\r\n                $(imgObj).css(\"height\",parseInt(h)+\"px\");\r\n               /*  imgObj.width=w; \r\n                imgObj.height=h;  */\r\n        }else{ \r\n            imgObj.width=image.width; \r\n            imgObj.height=image.height; \r\n        }  \r\n    } \r\n} \r\n\r\n$(document).ready(function(){\r\n     $(\".area\").hover(function(){\r\n            $(this).find(\".qq\").show(100);}\r\n            ,function(){\r\n               $(this).find(\".qq\").hide(100);\r\n     });\r\n\r\n     $(\".js .box01 a\").first().css('display',\"block\");\r\n          carousel(); \r\n     });\r\n});\r\n\r\n\r\nfunction DrawImage(imgObj,widthValue,heightValue){ \r\n    var image=new Image(); \r\n    image.src=imgObj.src;\r\n    if(image.width>0 && image.height>0){ \r\n        if(image.height>heightValue||image.width>widthValue){\r\n            var h=0,w,wflg=false;\r\n            if(image.height>heightValue)\r\n                wflg=true;\r\n            if(wflg){\r\n                w=widthValue; \r\n                h=(image.height*widthValue)/image.width; \r\n                }\r\n            if(h==0||h>heightValue){\r\n                h=heightValue; \r\n                w=(image.width*heightValue)/image.height; \r\n                }\r\n                //alert(w)\r\n                //alert(h)\r\n                $(imgObj).css(\"margin-left\",parseInt((widthValue-w)/2)+\"px\");\r\n                $(imgObj).css(\"margin-top\",parseInt((heightValue-h)/2)+\"px\");\r\n                $(imgObj).css(\"width\",parseInt(w)+\"px\");\r\n                $(imgObj).css(\"height\",parseInt(h)+\"px\");\r\n               /*  imgObj.width=w; \r\n                imgObj.height=h;  */\r\n        }else{ \r\n            imgObj.width=image.width; \r\n            imgObj.height=image.height; \r\n        }  \r\n    } \r\n} \r\n\r\n\r\nfunction carousel(){\r\n     var js_mun = $(\".js .box01 a\").length;\r\n     for(i=1;i<=js_mun;i++){\r\n          $(\".js .jsNav\").append('<a name=\"'+i+'\" href=\"javascript:void(0)\">'+i+'</a>');\r\n     }\r\n     $(\".js .jsNav a\").first().addClass('select');\r\n\r\n     $(\".prevBtn\").click(function(event) {\r\n          /* Act on the event */\r\n          var now_name = parseInt($(\".js .jsNav\").find(\".select\").attr('name'));\r\n          var tar_name = now_name - 1;\r\n          if(tar_name<=1){\r\n               tar_name = js_mun;\r\n          }\r\n          show_hide(tar_name);\r\n     });\r\n     $(\".nextBtn\").click(function(event) {\r\n          /* Act on the event */\r\n          var now_name = parseInt($(\".js .jsNav\").find(\".select\").attr('name'));\r\n          var tar_name = now_name + 1;\r\n          if(tar_name> js_mun){\r\n               tar_name = 1;\r\n          }\r\n          show_hide(tar_name);\r\n     });\r\n     $(\".jsNav\").delegate('a', 'mouseover', function(event) {\r\n          var tar_name = parseInt($(this).attr(\"name\"));\r\n          show_hide(tar_name);\r\n     });\r\n     setTimeout(function() {\r\n          settime();\r\n     }, 3000);\r\n}    \r\nfunction settime(){\r\n     var js_mun = $(\".js .box01 a\").length;\r\n     var now_name = parseInt($(\".js .jsNav\").find(\".select\").attr('name'));\r\n     var tar_name = now_name + 1;\r\n     if(tar_name> js_mun){\r\n          tar_name = 1;\r\n     }\r\n     show_hide(tar_name);\r\n     setTimeout(function() {\r\n          settime();\r\n     }, 3000);\r\n}\r\nfunction show_hide(tar_name){\r\n     $(\".js .jsNav a\").removeClass();\r\n     $(\".js .jsNav a[name='\"+tar_name+\"']\").addClass('select');\r\n     $(\".js .box01 a\").hide();\r\n     $(\".js .box01 a\").eq(tar_name-1).show();\r\n}\r\n\r\n</script>\r